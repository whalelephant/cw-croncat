        "type": "object",
        "required": [
          "agent_id",
          "payable_account_id"
        ],
        "properties": {
          "agent_id": {
            "type": "string"
          },
          "payable_account_id": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
