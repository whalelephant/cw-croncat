            "type": [
              "array",
              "null"
            ],
            "items": {
              "type": "string"
            }
          },
          "gas_price": {
            "anyOf": [
              {
                "$ref": "#/definitions/GasPrice"
              },
              {
                "type": "null"
              }
            ]
          },
          "owner_addr": {
            "type": [
              "string",
              "null"
            ]
          },
          "paused": {
            "type": [
              "boolean",
              "null"
            ]
          },
          "treasury_addr": {
            "type": [
              "string",
              "null"
            ]
          },
          "treasury_fee": {
            "type": [
              "integer",
              "null"
            ],
            "format": "uint64",
            "minimum": 0.0
          }
        },
        "additionalProperties": false
      },
